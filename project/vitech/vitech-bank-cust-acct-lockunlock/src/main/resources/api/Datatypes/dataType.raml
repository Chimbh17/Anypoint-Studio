#%RAML 1.0 DataType
type: object
properties:
  password:
    default: admin
  user:
    default: admin
  accountNum:
    required: true
    type: string
    minLength: 10
    maxLength: 10
  bank:
    required: true
    enum:
      - HDFC
      - AXIS
      - SBI
      - ICICI 
  acctStatus:
    required: true
    enum:
      - Lock
      - Unlock
#      - Active